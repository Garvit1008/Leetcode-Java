class Solution {
    public int heightChecker(int[] heights) {
        int count = 0;
        int a[] = new int[heights.length];
for(int i =0;i<heights.length;i++){
            a[i] = heights[i];
        }
        Arrays.sort(heights);
        for(int i =0;i<heights.length;i++){
            if(a[i]!=heights[i]){
                count++;
            }
        }
        return count;
    }
}
