class Solution {
    public String reverseWords(String s) {
        String a ="";
        String str[]=s.split(" ");
        for(int i =0;i<str.length;i++){
            for(int j=str[i].length()-1;j>=0;j--){
                a+=str[i].charAt(j);
            }
            if(i!=str.length-1){
                a+=" ";
            }            
        }
        return a;
    }
}
